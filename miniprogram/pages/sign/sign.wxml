<!--pages/sign.wxml-->
<view class="container_1">
<view class="title">填写报名信息</view>
<view class='mini'>
    请准确填写报名信息
</view>
<form bindsubmit="formSubmit" >
  <view class="weui-cells_after-title">
    <view class=" divLine"></view>
    <view class="weui-cell_input">
      <view class="weui-cell_hd">
        <view class="weui-label" style="float: left">姓名：</view>
        <input class="weui-input" value="{{list1.realname}}" name="realname" placeholder="请输入真实姓名"/>
      </view>
    </view>
    <view class='divLine'></view>

    <view class="weui-cell_input">
      <view class="weui-cell_hd">
        <view class="weui-label" style="float:left">学号：</view>
        <input class="weui-input" value="{{list1.student_id}}" name="sid" placeholder="请输入学号"/>
      </view>
    </view>
    <view class='divLine'></view>
   
    <view class="weui-cell_input">
      <view class="weui-cell_hd">
        <view class="weui-label" style="float:left">系别：</view>
        <input class="weui-input" value="{{list1.dept}}" name="dept" placeholder="请输入系别"/>
      </view>
    </view>
    <view class='divLine'></view>
    
    <view class="weui-cell_input">
      <view class="weui-cell_hd">
        <view class="weui-label" style="float:left">专业：</view>
        <input class="weui-input" value="{{list1.major}}" name="major" placeholder="请输入专业"/>
      </view>
    </view>
    <view class='divLine'></view>
    
    <view class="weui-cell_input">
      <view class="weui-cell_hd">
        <view class="weui-label" style="float:left">性别：</view>
        <input class="weui-input"  name="sex" value='{{list1.gender}}' name="sex" placeholder="请输入性别"/>
      </view>
    </view>
    <view class='divLine'></view>
    
    <view class="weui-cell_input">
      <view class="weui-cell_hd">
        <view class="weui-label" style="float:left">手机：</view>
        <input class="weui-input" value="{{list1.telephone}}" name="tel" placeholder="请输入手机号码"/>
      </view>  
    </view>
    <view class='divLine'></view>

    <view class="weui-cell_input">
      <view class="weui-cell_hd">
        <view class="weui-label" style="float:left">宿舍：</view>
        <input class="weui-input" value="{{list1.dormitory}}" name="dor" placeholder="请输入宿舍号"/>
      </view>
    </view>
  </view>

</form>
<view class='divLine lastDiv'></view>
</view>
  <view class="btn">
     <button  class="weui-btn"  formType="submit">提交</button>
  </view>

